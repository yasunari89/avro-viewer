{
  "name": "avro-viewer",
  "displayName": "avro-viewer",
  "description": "This is a vscode extension that allows you to preview .avro files.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "avro-viewer.open",
        "title": "Open Avro"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "when": "resourceExtname == .avro",
          "command": "avro-viewer.open",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "avsc": "^5.7.7"
  },
  "repository": {
    "url": "https://github.com/yasunari89/avro-viewer/"
  },
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": {
    "name": "Yasunari Ota",
    "url": "https://github.com/yasunari89/"
  }
}